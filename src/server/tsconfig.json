{
    "env": {
      "node": true,
      "es6": true,
      "browser": true,
    },
    "globals": {},
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "types": ["node"],
      "target": "es6",
      "lib": [
        "es6",
        "dom"
      ],
      "noImplicitAny": false,
      "sourceMap": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "moduleResolution": "node",
      // "noUnusedLocals": true
    },
    "compileOnSave": false,
    "exclude": [
      "node_modules"
    ]
  }